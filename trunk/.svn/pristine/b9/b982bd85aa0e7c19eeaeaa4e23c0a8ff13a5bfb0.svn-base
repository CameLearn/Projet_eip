<VirtualHost *:80>
    ServerAdmin webmaster@localhost
    ServerName www.camelearn.dev
    ServerAlias camelearn.dev
    DocumentRoot "/var/www/web"
    <Directory "/var/www/web">
        Options Indexes FollowSymLinks
        Order Allow,Deny
        Allow from All
        AllowOverride All
    </Directory>
    ErrorLog "/var/log/apache2/camelearn-error_log"
    CustomLog "/var/log/apache2/camelearn-access_log" common
</VirtualHost>